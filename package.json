{
    "name": "pr-log",
    "version": "1.4.0",
    "description": "Changelog generator based on GitHub Pull Requests",
    "bin": "dist/bin/pr-log.js",
    "scripts": {
        "build": "babel lib --out-dir dist && babel bin --out-dir dist/bin",
        "pretest": "eslint .",
        "test": "npm run test:unit --coverage",
        "posttest": "npm run check-coverage",
        "prepublish": "npm run build",
        "test:unit": "istanbul cover _mocha -- --compilers js:babel-register test",
        "check-coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
        "coveralls": "cat ./build/coverage/lcov.info | coveralls"
    },
    "author": "Mathias Schreck <schreck.mathias@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "bluebird": "3.3.5",
        "commander": "2.9.0",
        "git-promise": "0.3.0",
        "ramda": "0.21.0",
        "moment": "2.13.0",
        "git-url-parse": "6.0.2",
        "parse-github-repo-url": "1.3.0",
        "prepend": "1.0.2",
        "semver": "5.1.0",
        "restling": "0.9.1"
    },
    "devDependencies": {
        "babel-cli": "6.7.7",
        "babel-preset-es2015": "6.6.0",
        "babel-register": "6.7.2",
        "chai": "3.5.0",
        "chai-as-promised": "5.3.0",
        "coveralls": "2.11.9",
        "eslint": "2.9.0",
        "eslint-config-holidaycheck": "0.7.0",
        "eslint-plugin-mocha": "2.2.0",
        "istanbul": "1.0.0-alpha.2",
        "istanbul-api": "lo1tuma/istanbul-api#435b30574137852fc778044f2973624aa9c9184d",
        "mocha": "2.4.5",
        "proxyquire": "1.7.4",
        "sinon": "1.17.3",
        "sinon-as-promised": "4.0.0",
        "sinon-chai": "2.8.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lo1tuma/pr-log.git"
    },
    "keywords": [
        "pr-log",
        "changelog",
        "changelog.md",
        "github",
        "history",
        "history.md"
    ],
    "contributors": [
        "Alexander Schmidt <alexanderschmidt1@gmail.com>"
    ]
}
