{
    "name": "pr-log",
    "version": "1.4.0",
    "description": "Changelog generator based on GitHub Pull Requests",
    "bin": "bin/pr-log.js",
    "scripts": {
        "pretest": "eslint .",
        "test": "npm run test:unit --coverage",
        "posttest": "npm run check-coverage",
        "test:unit": "istanbul test _mocha -- test",
        "check-coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
        "coveralls": "cat ./build/coverage/lcov.info | coveralls"
    },
    "author": "Mathias Schreck <schreck.mathias@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "bluebird": "3.3.4",
        "commander": "2.9.0",
        "git-promise": "0.3.0",
        "ramda": "0.19.1",
        "moment": "2.12.0",
        "git-url-parse": "6.0.1",
        "parse-github-repo-url": "1.0.0",
        "prepend": "1.0.2",
        "semver": "5.1.0",
        "restling": "0.9.1"
    },
    "devDependencies": {
        "chai": "3.5.0",
        "chai-as-promised": "5.2.0",
        "coveralls": "2.11.8",
        "eslint": "2.3.0",
        "eslint-config-holidaycheck": "0.4.0",
        "eslint-plugin-mocha": "2.0.0",
        "istanbul": "0.4.2",
        "mocha": "2.4.5",
        "proxyquire": "1.7.4",
        "sinon": "1.17.3",
        "sinon-as-promised": "4.0.0",
        "sinon-chai": "2.8.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lo1tuma/pr-log.git"
    },
    "keywords": [
        "pr-log",
        "changelog",
        "changelog.md",
        "github",
        "history",
        "history.md"
    ],
    "contributors": [
        "Alexander Schmidt <alexanderschmidt1@gmail.com>"
    ]
}
